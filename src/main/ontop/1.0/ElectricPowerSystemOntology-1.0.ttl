  rdfs:subClassOf [ owl:onProperty seas:evaluationOf ; owl:someValuesFrom seas:ElectricEnergyProperty ] ;
  owl:equivalentClass [ owl:onProperty seas:evaluationOf ; owl:someValuesFrom seas:ElectricEnergyProperty ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:ElectricPowerSystemOntology .


seas:electricEnergy a owl:ObjectProperty ;
  rdfs:label "electric energy" ;
  rdfs:comment """Links a feature of interest to one of its electric energy properties.

There may be many energy properties for a feature of interest, so this property is not functional."""@en ;
  rdfs:subPropertyOf seas:hasProperty ;
  rdfs:range seas:ElectricEnergyProperty ; 
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:ElectricPowerSystemOntology .

  # for electric energy systems

  seas:consumedElectricEnergy a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "consumed electric energy" ;
    rdfs:comment """Links an electric energy system to the consumed electric energy."""@en ;
    rdfs:subPropertyOf seas:hasProperty , seas:electricEnergy ;
    rdfs:domain seas:ElectricPowerSystem ;
    rdfs:range seas:ElectricEnergyProperty ;
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemOntology .

  seas:producedElectricEnergy a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "produced electric energy" ;
    rdfs:comment """Links an electric energy system to the produced electric energy."""@en ;
    rdfs:subPropertyOf seas:hasProperty , seas:electricEnergy ;
    rdfs:domain seas:ElectricPowerSystem ;
    rdfs:range seas:ElectricEnergyProperty ;
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemOntology .

  seas:storedElectricEnergy a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "stored electric energy" ;
    rdfs:comment """Links an electric energy system to the stored electric energy."""@en ;
    rdfs:subPropertyOf seas:hasProperty , seas:electricEnergy ;
    rdfs:domain seas:ElectricPowerSystem ;
    rdfs:range seas:ElectricEnergyProperty ;
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemOntology .

  # for electric energy systems or electrical connection points

  seas:incomingElectricEnergy a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "incoming electric energy" ;
    rdfs:comment """Links an electric energy system, or an electrical connection point, to the incoming electric energy.
    (a) the sum of the electric energy that enter the system from the exterior, or
    (b) the electric energy that enters the system through this connection point."""@en ;
    rdfs:subPropertyOf seas:hasProperty , seas:electricEnergy ;
    rdfs:domain [ owl:unionOf ( seas:ElectricPowerSystem seas:ElectricalConnectionPoint ) ] ;
    rdfs:range seas:ElectricEnergyProperty ;
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemOntology .

  seas:outgoingElectricEnergy a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "outgoing electric energy" ;
    rdfs:comment """Links an electric energy system, or an electrical connection point, to the outgoing electric energy.
    (a) the sum of the electric energy that leaves the system from the exterior, or
    (b) the electric energy that leaves the system through this connection point."""@en ;
    rdfs:subPropertyOf seas:hasProperty , seas:electricEnergy ;
    rdfs:domain [ owl:unionOf ( seas:ElectricPowerSystem seas:ElectricalConnectionPoint ) ] ;
    rdfs:range seas:ElectricEnergyProperty ;
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemOntology .

  seas:electricEnergyBalance a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:label "electric energy balance" ;
    rdfs:comment """Links an electric energy system, or an electrical connection point, to the balance between the incoming electric energy and the outgoing electric energy."""@en ;
    rdfs:subPropertyOf seas:hasProperty , seas:electricEnergy ;
    rdfs:domain [ owl:unionOf ( seas:ElectricPowerSystem seas:ElectricalConnectionPoint ) ] ;
    rdfs:range seas:ElectricEnergyProperty ;
    vs:term_status "testing" ;
    rdfs:isDefinedBy seas:ElectricPowerSystemOntology .


