seas:SingleBusSystemVocabulary a owl:Ontology , voaf:Vocabulary ;
  dc:title "seas-Single Bus System vocabulary."@en ;
  dc:description """This vocabulary defines Single Bus Systems for the Power and Energy Systems domain. 
  These are sistems connected to a single bus, such as: Loads, Generators or Storages"""@en ;
  dc:issued "2016-07-20"^^xsd:date ;
  dc:modified "2016-07-20"^^xsd:date ;
  dc:contributor <http://www.maxime-lefrancois.info/me#> ;
  dc:author <https://w3id.org/people/gabrieljlsantos#> ;
  dc:contributor <https://w3id.org/people/franciscosilva#> ;
  dc:contributor <https://w3id.org/people/brigidateixeira#> ;
  cc:license <http://creativecommons.org/licenses/by/4.0/> ;
  vann:preferredNamespacePrefix "seas" ;
  vann:preferredNamespaceUri <https://w3id.org/seas/> ;  
  owl:versionIRI <https://w3id.org/seas/SingleBusSystemVocabulary/0.10> ;
  owl:versionInfo "v0.10" .

seas:SingleBusSystem a owl:Class, owl:disjointUnionOf ( seas:Load seas:Generator seas:Storage ) ;
  rdfs:subClassOf seas:PowerSystem ;
  rdfs:label "Single Bus System"@en ;
  rdfs:label "Sistema de Barramento Ãºnico"@pt ;
  rdfs:comment """The class of systems that are connected to a single Bus."""@en ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy seas:SingleBusSystemVocabulary .
